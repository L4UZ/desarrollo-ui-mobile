{
  "expo": {
    "name": "uTravel",
    "slug": "utravel",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo192.png",
    "scheme": "myapp",
    "splash": {
      "image": "./src/assets/images/logo512.png",
      "resizeMode": "contain",
      "backgroundColor": "#3f51b5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./src/assets/images/logo.svg"
    }
  }
}
